	Keywords = {Electrospinning,Endothelial cell,Fluid flow,Folder - Drug Delivery Concept/Principles - polyme,Scaffold,Vascular tissue engineering},
	Mendeley-Tags = {Folder - Drug Delivery Concept/Principles - polyme},
	Month = {jan},
