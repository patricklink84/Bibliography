	Journal = {BioMed research international},
	Pages = {923475},
	Pmid = {23509805},
	Publisher = {Hindawi Publishing Corporation},
	Title = {{Prior Lung Inflammation Impacts on Body Distribution of Gold Nanoparticles}},
