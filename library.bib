	Keywords = {Bioconjugate,Collagen like peptide,Folder - hydrogels,Hydrogel,Hydrogels,Self-assembly,Triple helix},
	Mendeley-Tags = {Folder - hydrogels,Hydrogels},
	Month = {oct},
	Number = {10},
	Pages = {2998--3009},
	Pmid = {24039275},
