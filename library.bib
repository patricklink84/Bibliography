	Journal = {American Journal of Respiratory and Critical Care Medicine},
	Keywords = {Aerosols -- Pharmacology,Air Pollutants -- Pharmacokinetics,Air pollution,Carbon -- Pharmacokinetics,Chronic Obstructive -- Metabolism,Clearance,Lung -- Metabolism,Pulmonary Disease,Translocation,Ultrafine particles},
	Mendeley-Tags = {Aerosols -- Pharmacology,Air Pollutants -- Pharmacokinetics,Carbon -- Pharmacokinetics,Chronic Obstructive -- Metabolism,Lung -- Metabolism,Pulmonary Disease},
