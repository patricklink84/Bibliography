	Keywords = {Administration,Aerosols,Animals,Biological Transport,Brain,Brain: metabolism,Feces,Feces: chemistry,Gamma,Inbred WKY,Inhalation,Iridium,Iridium: administration {\{}{\&}{\}} dosage,Iridium: pharmacokinetics,Liver,Liver: metabolism,Lung,Lung: metabolism,Male,Myocardium,Myocardium: metabolism,Particle Size,Rats,Spectrometry,Spleen,Spleen: metabolism,Tissue Distribution,Urinalysis},
