@article{Kohn-Luque2011Early-Embryonic,
	Author = {K{\"o}hn-Luque, Alvaro and de Back, Walter and Starru{\ss}, J{\"o}rn and Mattiotti, Andrea and Deutsch, Andreas and P{\'e}rez-Pomares, Jos{\'e}Mar{\'\i}a and Herrero, Miguel A.},
