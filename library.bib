	Journal = {Journal of the National Cancer Institute},
	Number = {12},
	Pages = {812--824},
	Pmid = {16788155},
	Title = {{Keratinocyte growth factor expression and activity in cancer: Implications for use in patients with solid tumors}},
	Url = {http://jnci.oxfordjournals.org.proxy.library.vcu.edu/content/98/12/812},
	Volume = {98},
