@article{Zhang2015Effect-of-Polye,
	Author = {Zhang, He and Chang, Hao and Wang, Li-Mei and Ren, Ke-Feng and Martins, M Cristina L and Barbosa, M{\'a}rio A and Ji, Jian},
	Date-Added = {2016-04-26 00:21:50 +0000},
