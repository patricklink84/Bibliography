@article{Kaku2014Overexpression-,
	Author = {Kaku, Yoichiro and Imaoka, Haruki and Morimatsu, Yoshitaka and Komohara, Yoshihiro and Ohnishi, Koji and Oda, Hanako and Takenaka, Shinichi and Matsuoka, Masanobu and Kawayama, Tomotaka and Takeya, Motohiro and Hoshino, Tomoaki},
