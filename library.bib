@article{Cooley2014TGF-b-Signaling,
	Abstract = {Veins grafted into an arterial environment undergo a complex vascular remodeling process. Pathologic vascular remodeling often results in stenosed or occluded conduit grafts. Understanding this complex process is important for improving the outcome of patients with coronary and peripheral artery disease undergoing surgical revascularization. Using in vivo murine cell lineage{\textendash}tracing models, we show that endothelial-derived cells contribute to neointimal formation through endothelial-to-mesenchymal transition (EndMT), which is dependent on early activation of the Smad2/3-Slug signaling pathway. Antagonism of transforming growth factor{\textendash}β (TGF-β) signaling by TGF-β neutralizing antibody, short hairpin RNA{\textendash}mediated Smad3 or Smad2 knockdown, Smad3 haploinsufficiency, or endothelial cell{\textendash}specific Smad2 deletion resulted in decreased EndMT and less neointimal formation compared to controls. Histological examination of postmortem human vein graft tissue corroborated the changes observed in our mouse vein graft model, suggesting that EndMT is operative during human vein graft remodeling. These data establish that EndMT is an important mechanism underlying neointimal formation in interpositional vein grafts, and identifies the TGF-β{\textendash}Smad2/3{\textendash}Slug signaling pathway as a potential therapeutic target to prevent clinical vein graft stenosis.},
	Author = {Cooley, Brian C. and Nevado, Jose and Mellad, Jason and Yang, Dan and St. Hilaire, Cynthia and Negro, Alejandra and Fang, Fang and Chen, Guibin and San, Hong and Walts, Avram D. and Schwartzbeck, Robin L. and Taylor, Brandi and Lanzer, Jan D. and Wragg, Andrew and Elagha, Abdalla and Beltran, Leilani E. and Berry, Colin and Feil, Robert and Virmani, Renu and Ladich, Elena and Kovacic, Jason C. and Boehm, Manfred},
