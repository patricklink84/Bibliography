	Keywords = {HepG2,Hepatocytes,POSS-modified polycaprolactone urea urethane,Tissue Engineering,hepatocytes,nanocomposite polymers,porogens,poss-modified polycaprolactone urea urethane,scaffolds,tissue engineering},
	Mendeley-Tags = {HepG2,Hepatocytes,POSS-modified polycaprolactone urea urethane,Tissue Engineering,nanocomposite polymers,porogens,scaffolds},
