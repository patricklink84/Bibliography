	Journal = {Molecular Biology of the Cell},
	Number = {12},
	Pages = {4295-4308},
	Title = {The Latent Transforming Growth Factor-β--binding Protein-1 Promotes In Vitro Differentiation of Embryonic Stem Cells into Endothelium},
