	Journal = {Journal of Biomedical Materials Research Part A},
	Keywords = {biologic scaffold, bioscaffold, macrophages, extracellular matrix, host response},
	Number = {1},
	Pages = {138--147},
	Title = {Solubilized extracellular matrix bioscaffolds derived from diverse source tissues differentially influence macrophage phenotype},
