@article{Valero2014A-Cell-Regulato,
	Author = {Valero, Clara and Javierre, Etelvina and Garc{\'\i}a-Aznar, Jos{\'e}Manuel and G{\'o}mez-Benito, Mar{\'\i}a Jos{\'e}},
	Date = {2014/03/28},
	Date-Added = {2017-08-17 23:04:12 +0000},
