	Pmid = {23815236},
	Title = {{Characterization of a Liver Organoid Tissue Composed of Hepatocytes and Fibroblasts in Dense Collagen Fibrils}},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3807535/$\backslash$nhttp://www.ncbi.nlm.nih.gov/pmc/articles/PMC3807535/pdf/ten.tea.2012.0704.pdf http://online.liebertpub.com.proxy.library.vcu.edu/doi/abs/10.1089/ten.TEA.2012.0704 http://online.liebertpub.com.proxy.lib},
