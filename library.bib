	Issn = {19326203},
	Journal = {PLoS ONE},
	Number = {12},
	Pages = {1--12},
	Pmid = {24367590},
	Title = {{Keratinocyte growth factor gene delivery via mesenchymal stem cells protects against lipopolysaccharide-induced acute lung injury in mice}},
	Volume = {8},
	Year = {2013},
