	Isbn = {09284931},
	Issn = {09284931},
	Journal = {Materials Science and Engineering C},
	Keywords = {3D,Calcium phosphate,Calcium sulfate,Drop penetration,Folder - 3d printing,Particle size,Printing,Three-dimensional printing (3DP),Tissue engineering},
	Mendeley-Tags = {3D,Folder - 3d printing,Printing},
