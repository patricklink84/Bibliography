	Note = {Copyright - Copyright Nature Publishing Group Nov 2015; Last updated - 2016-04-14},
	Pages = {15014},
	Title = {Combinatorial interventions inhibit TGFbeta]-driven epithelial-to-mesenchymal transition and support hybrid cellular phenotypes},
	Url = {http://proxy.library.vcu.edu/login?url=https://search.proquest.com/docview/1780727026?accountid=14780},
